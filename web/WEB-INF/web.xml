<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <welcome-file-list>
        <welcome-file>reportChart.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>vehModelSalServlet</servlet-name>
        <servlet-class>servlet.VehModelSalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vehMonthSalServlet</servlet-name>
        <servlet-class>servlet.VehMonthSalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vehWorkingRateServlet</servlet-name>
        <servlet-class>servlet.VehWorkingRateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vehModelOverloadServlet</servlet-name>
        <servlet-class>servlet.VehModelOverloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sendsms</servlet-name>
        <servlet-class>servlet.Sendsms</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>vehModelSalServlet</servlet-name>
        <url-pattern>/vehModelSal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vehMonthSalServlet</servlet-name>
        <url-pattern>/vehMonthSal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vehWorkingRateServlet</servlet-name>
        <url-pattern>/vehWorkingRate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vehModelOverloadServlet</servlet-name>
        <url-pattern>/vehModelOverload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sendsms</servlet-name>
        <url-pattern>/sendsms</url-pattern>
    </servlet-mapping>
</web-app>